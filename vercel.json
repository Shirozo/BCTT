{
    "builds": [
        {
            "src": "bctt/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
              "distDir": "staticfiles"
            }
          }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "bctt/wsgi.py"
        },
        {
            "src": "/staticfiles/(.*)",
            "dest": "/staticfiles/$1"
        }
    ]
}